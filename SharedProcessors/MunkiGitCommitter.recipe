<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Commits changes to Git Production Branch</string>
    <key>Identifier</key>
    <string>com.github.grahampugh.recipes.MunkiGitCommitter</string>
    <key>Input</key>
    <dict/>
    <key>MinimumVersion</key>
    <string>0.4.0</string>
    <key>Process</key>
    <array>
        <dict> 
            <key>Processor</key> 
            <string>MunkiGitCommitter</string> 
            <key>Arguments</key>
            <dict>
                <key>MUNKI_REPO</key>
                <string>%MUNKI_REPO%</string>
                <key>PRODUCTION_BRANCH</key>
                <string>master</string>
            </dict>
        </dict> 
    </array>
</dict>
</plist>